{
  "expo": {
    "name": "Healthier",
    "slug": "Healthier",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "host.exp.exponent",
    "owner": "pablo_mds",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "platforms": ["ios", "android", "web"],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "host.exp.exponent",
      "infoPlist": {
      "CFBundleURLTypes": [
        {
          "CFBundleURLSchemes": ["com.googleusercontent.apps.70429617594-2os8mc39s6fnb09nslq7o6n87822mkh3 "]
        }
      ]
    }
    },
    "intentFilters": [
      {
        "action": "VIEW",
        "data": {
          "scheme": "https",
          "host": "auth.expo.io",
          "pathPrefix": "/@pablo_mds/Healthier"
        },
        "category": ["BROWSABLE", "DEFAULT"]
      }
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "host.exp.exponent",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset", 
      "expo-font",
      "expo-dev-client"
    ],
    "extra": {
      "eas": {
        "projectId": "6f48f71c-d736-497e-b16d-3d287f0bf695"
      }
    }
  }
}
